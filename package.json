{
  "name": "contentful-config",
  "version": "1.0.0",
  "description": "A package for managing Contentful configurations",
  "keywords": [
    "typescript",
    "contentful",
    "cms",
    "configuration"
  ],
  "homepage": "https://github.com/aweissman11/contentful-config",
  "bugs": {
    "url": "https://github.com/aweissman11/contentful-config/issues"
  },
  "author": "Aaron Weissman",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aweissman11/contentful-config.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "ci": "npm run build && npm run check-format && npm run check-exports && npm run test",
    "format": "prettier --write .",
    "check-format": "prettier --check .",
    "check-exports": "attw --pack . --ignore-rules=cjs-resolves-to-esm",
    "test": "vitest run",
    "dev": "vitest",
    "local-release": "changeset version && changeset publish",
    "prepublishOnly": "npm run ci"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@changesets/cli": "^2.29.4",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "main": "dist/index.js"
}
